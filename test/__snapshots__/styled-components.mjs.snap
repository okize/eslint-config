{
  "language": "@/js",
  "languageOptions": {
    "ecmaVersion": 2026,
    "parser": "espree@10.4.0",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true,
      },
      "sourceType": "module",
    },
    "sourceType": "module",
  },
  "linterOptions": {
    "reportUnusedDisableDirectives": 1,
  },
  "plugins": [
    "@",
    "styled-components-a11y:eslint-plugin-styled-components-a11y@2.2.1",
  ],
  "rules": {
    "styled-components-a11y/accessible-emoji": [
      2,
    ],
    "styled-components-a11y/alt-text": [
      2,
    ],
    "styled-components-a11y/anchor-has-content": [
      2,
    ],
    "styled-components-a11y/anchor-is-valid": [
      2,
    ],
    "styled-components-a11y/aria-activedescendant-has-tabindex": [
      2,
    ],
    "styled-components-a11y/aria-props": [
      2,
    ],
    "styled-components-a11y/aria-proptypes": [
      2,
    ],
    "styled-components-a11y/aria-role": [
      2,
    ],
    "styled-components-a11y/aria-unsupported-elements": [
      2,
    ],
    "styled-components-a11y/autocomplete-valid": [
      2,
    ],
    "styled-components-a11y/click-events-have-key-events": [
      2,
    ],
    "styled-components-a11y/control-has-associated-label": [
      2,
      {
        "ignoreElements": [
          "audio",
          "canvas",
          "embed",
          "input",
          "textarea",
          "tr",
          "video",
        ],
        "ignoreRoles": [
          "grid",
          "listbox",
          "menu",
          "menubar",
          "radiogroup",
          "row",
          "tablist",
          "toolbar",
          "tree",
          "treegrid",
        ],
        "includeRoles": [
          "alert",
          "dialog",
        ],
      },
    ],
    "styled-components-a11y/heading-has-content": [
      2,
    ],
    "styled-components-a11y/html-has-lang": [
      2,
    ],
    "styled-components-a11y/iframe-has-title": [
      2,
    ],
    "styled-components-a11y/img-redundant-alt": [
      2,
    ],
    "styled-components-a11y/interactive-supports-focus": [
      2,
      {
        "tabbable": [
          "button",
          "checkbox",
          "link",
          "progressbar",
          "searchbox",
          "slider",
          "spinbutton",
          "switch",
          "textbox",
        ],
      },
    ],
    "styled-components-a11y/label-has-associated-control": [
      2,
    ],
    "styled-components-a11y/media-has-caption": [
      2,
    ],
    "styled-components-a11y/mouse-events-have-key-events": [
      2,
    ],
    "styled-components-a11y/no-access-key": [
      2,
    ],
    "styled-components-a11y/no-autofocus": [
      2,
    ],
    "styled-components-a11y/no-distracting-elements": [
      2,
    ],
    "styled-components-a11y/no-interactive-element-to-noninteractive-role": [
      2,
    ],
    "styled-components-a11y/no-noninteractive-element-interactions": [
      2,
      {
        "body": [
          "onError",
          "onLoad",
        ],
        "iframe": [
          "onError",
          "onLoad",
        ],
        "img": [
          "onError",
          "onLoad",
        ],
      },
    ],
    "styled-components-a11y/no-noninteractive-element-to-interactive-role": [
      2,
    ],
    "styled-components-a11y/no-noninteractive-tabindex": [
      2,
    ],
    "styled-components-a11y/no-onchange": [
      2,
    ],
    "styled-components-a11y/no-redundant-roles": [
      2,
    ],
    "styled-components-a11y/no-static-element-interactions": [
      2,
    ],
    "styled-components-a11y/role-has-required-aria-props": [
      2,
    ],
    "styled-components-a11y/role-supports-aria-props": [
      2,
    ],
    "styled-components-a11y/scope": [
      2,
    ],
    "styled-components-a11y/tabindex-no-positive": [
      2,
    ],
  },
}